<div class="page-header">
  <%= link_to new_alpha_framework_def_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Alpha framework def
  <% end %>
  <h1>Listing alpha_framework_defs</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Dname</th>
            <th>Ddescription</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @alpha_framework_defs) do |alpha_framework_def| %>
            <td><%= alpha_framework_def.dname %></td>
            <td><%= alpha_framework_def.ddescription %></td>
            <td><%= link_to 'Show', alpha_framework_def %></td>
        <td><%= link_to 'Edit', edit_alpha_framework_def_path(alpha_framework_def) %></td>
        <td><%= link_to 'Destroy', alpha_framework_def, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
