<div class="page-header">
  <%= link_to new_alpha_framework_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Alpha framework
  <% end %>
  <h1>Listing alpha_frameworks</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Project</th>
            <th>Alpha framework def</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @alpha_frameworks) do |alpha_framework| %>
            <td><%= alpha_framework.project %></td>
            <td><%= alpha_framework.alpha_framework_def %></td>
            <td><%= link_to 'Show', alpha_framework %></td>
        <td><%= link_to 'Edit', edit_alpha_framework_path(alpha_framework) %></td>
        <td><%= link_to 'Destroy', alpha_framework, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
