<div class="page-header">
  <%= link_to alpha_items_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_alpha_item_path(@alpha_item), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show alpha_item</h1>
</div>

<dl class="dl-horizontal">
  <dt>Completed:</dt>
  <dd><%= @alpha_item.completed %></dd>

  <dt>Completed at:</dt>
  <dd><%= @alpha_item.completed_at %></dd>

  <dt>Alpha state:</dt>
  <dd><%= @alpha_item.alpha_state %></dd>

  <dt>Alpha item def:</dt>
  <dd><%= @alpha_item.alpha_item_def %></dd>

</dl>
