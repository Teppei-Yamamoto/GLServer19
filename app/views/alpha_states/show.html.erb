<div class="page-header">
  <%= link_to alpha_states_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_alpha_state_path(@alpha_state), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show alpha_state</h1>
</div>

<dl class="dl-horizontal">
  <dt>Completed:</dt>
  <dd><%= @alpha_state.completed %></dd>

  <dt>Completed items:</dt>
  <dd><%= @alpha_state.completed_items %></dd>

  <dt>Completed at:</dt>
  <dd><%= @alpha_state.completed_at %></dd>

  <dt>Alpha alpha:</dt>
  <dd><%= @alpha_state.alpha_alpha %></dd>

  <dt>Alpha state def:</dt>
  <dd><%= @alpha_state.alpha_state_def %></dd>

</dl>
