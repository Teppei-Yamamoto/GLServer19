<div class="page-header">
  <h1><%= @alpha_alpha %></h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>No.</th>
        <th>Alpha state</th>
        <th>Completed</th>
        <th>Completed items</th>
        <th>Completed at</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @alpha_states) do |alpha_state| %>
            <td><%= alpha_state.dseq %></td>
            <td><%= link_to alpha_state.dname,  alpha_state_alpha_items_path(alpha_state) %></td>
            <td><%= alpha_state.completed %></td>
            <td><%= alpha_state.completed_items %></td>
            <td><%= alpha_state.completed_at %></td>
      <% end %>
    </tbody>
  </table>
</div>
