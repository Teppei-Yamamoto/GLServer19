<div class="page-header">
  <h1>アルファのリスト</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>No.</th>
        <th>アルファ名</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @alpha_alphas) do |alpha_alpha| %>
        <td><%= alpha_alpha.dseq %></td>
        <td><%= link_to alpha_alpha.dname,  alpha_alpha_alpha_states_path(alpha_alpha) %></td>
      <% end %>
    </tbody>
  </table>
</div>
