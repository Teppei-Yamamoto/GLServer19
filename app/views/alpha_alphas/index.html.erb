<div class="page-header">
  <%= link_to new_alpha_alpha_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Alpha alpha
  <% end %>
  <h1>Listing alpha_alphas</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Alpha framework</th>
            <th>Alpha alpha def</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @alpha_alphas) do |alpha_alpha| %>
            <td><%= alpha_alpha.alpha_framework %></td>
            <td><%= alpha_alpha.alpha_alpha_def %></td>
            <td><%= link_to 'Show', alpha_alpha %></td>
        <td><%= link_to 'Edit', edit_alpha_alpha_path(alpha_alpha) %></td>
        <td><%= link_to 'Destroy', alpha_alpha, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
