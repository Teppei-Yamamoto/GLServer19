<div class="page-header">
  <%= link_to new_sprintdiary_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Sprintdiary
  <% end %>
  <h1>Listing sprintdiaries</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Surum menber</th>
            <th>Bdiary</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @sprintdiaries) do |sprintdiary| %>
            <td><%= sprintdiary.surum_menber %></td>
            <td><%= sprintdiary.bdiary %></td>
            <td><%= link_to 'Show', sprintdiary %></td>
        <td><%= link_to 'Edit', edit_sprintdiary_path(sprintdiary) %></td>
        <td><%= link_to 'Destroy', sprintdiary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
